{
	"inputs": {
		"rootfs": "catalog:distro.warpforge.org/bootstrap-rootfs:focal-1645037482:x86_64"
	},
	"steps": {
		"build": {
			"protoformula": {
				"inputs": {
					"/": "pipe::rootfs",
					"/src": "catalog:gnu.org/make:v4.3:src"
				},
				"action": {
					"script": {
						"interpreter": "/bin/sh",
						"contents": [
							"cd /src/*",
							"mkdir -v build",
							"cd build"
							"../configure --prefix=/prefix/"
							"make -j3",
							"make DESTDIR=/out install",
						]
					}
				},
				"outputs": {
					"out": {
						"from": "/out/prefix",
						"packtype": "tar"
					}
				}
			}
		}
	},
	"outputs": {
		"x86_64": "pipe:build:out"
	}
}
